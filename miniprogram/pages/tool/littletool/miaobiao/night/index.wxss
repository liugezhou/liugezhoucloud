width:100%
}
/* button */
.myButton {
	box-shadow: 0px 10px 14px -7px #3e7327;
	background:linear-gradient(to bottom, #518bbb 5%, #5163b3 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#518bbb', endColorstr='#5163b3',GradientType=0);
	background-color:#4fa8d1;
	border-radius:4px;
	border:1px solid #4b8f29;
	display:inline-block;
	cursor:pointer;
	color:#0f0404;
	font-family:Arial;
	font-size:13px;
	font-weight:bold;
	padding:6px 12px;
	text-decoration:none;
	text-shadow:0px 1px 0px #5b8a3c;
}
.myButton:hover {
	background:linear-gradient(to bottom, #4e8dc0 5%, #495db6 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#4e8dc0', endColorstr='#495db6',GradientType=0);
	background-color:#5271c5;
}
.myButton:active {
	position:relative;
	top:1px;
}
/* button */
