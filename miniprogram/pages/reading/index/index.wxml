<scroll-view scroll-y class="scrollPage">
  <image src='http://img.liugezhou.online/20190722airplane.jpg' mode='widthFix' class='png' style='width:100%;height:486rpx'></image>
  <official-account></official-account>
  <view class="cu-card">
    <view class="cu-item bg-img shadow-blur" style="background-image:url({{item.img}})" bindtap="toChild" data-url="{{item.url}}" wx:for="{{list}}" wx:key>
      <view class="cardTitle">
        {{item.title}}
      </view>
    </view>
  </view>
  <view class='cu-tabbar-height'></view>
</scroll-view>